{
  "name": "my-store",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "start": "node index.js",
    "lint": "eslint ",
    "lint:fix": "eslint --fix",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "e2e": "NODE_ENV=e2e jest --config ./jest-e2e.json --verbose --forceExit --runInBand",
    "e2e:ci": "NODE_ENV=ci jest --config ./jest-e2e.json --verbose --forceExit --runInBand",
    "e2e:coverage": "NODE_ENV=e2e jest --config ./jest-e2e.json --verbose --forceExit --coverage",
    "seed:all": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "sequelize": "sequelize-cli"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "eslint": "^9.5.0",
    "globals": "^15.6.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.2",
    "prettier": "^2.8.8",
    "supertest": "^7.0.0"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@hapi/boom": "^10.0.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "eslint-plugin-jest": "^28.6.0",
    "express": "^4.19.2",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.13",
